{
  "name": "blog",
  "description": "Phillip Maier's personal blog/website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:doc": "docker build -t blog .",
    "dev:doc": "docker run --rm --name personal-blog -p 3000:3000 -v $INIT_CWD:/usr/src blog",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy:aws": "next build && next export && aws s3 cp --recursive ./out s3://phillipmaier.com",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rm -rf node_modules/ & rm -rf .next/ & rm -rf out/ & rm package-lock.json",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "next": "^11.0.1",
    "next-seo": "^4.26.0",
    "react": "^17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@next/bundle-analyzer": "^11.0.1",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.12",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "babel-plugin-styled-components": "^1.13.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{tsx,ts,js,jsx}": [
      "npm run lint"
    ]
  }
}
